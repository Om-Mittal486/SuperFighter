}

.upper-button-style {
    flex-shrink: 0;
    width: 32px;
    height: 32px;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
    margin-left: 3px;
    margin-right: 3px;
